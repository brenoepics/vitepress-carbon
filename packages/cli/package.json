{
  "name": "vpcar",
  "version": "1.4.1",
  "description": "VitePress Carbon CLI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brenoepics/vitepress-carbon.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/vpcar.mjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "vpcar": "./bin/vpcar.mjs",
    "vp-car": "./bin/vpcar.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev:prepare": "unbuild --stub",
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "cli": "node ./bin/vpcar.mjs",
    "vpcar": "node ./bin/vpcar.mjs",
    "vpcar-bun": "bun --bun ./bin/vpcar.mjs",
    "prepack": "unbuild",
    "release": "npm publish"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/semver": "^7.5.8",
    "citty": "^0.1.6",
    "consola": "^3.4.0",
    "execa": "^9.5.1",
    "giget": "^1.2.3",
    "nypm": "^0.3.12",
    "pathe": "^2.0.2",
    "semver": "^7.6.3",
    "unbuild": "^2.0.0",
    "unplugin-purge-polyfills": "^0.0.7",
    "yaml": "^2.6.0",
    "rollup": "^4.28.0"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": "^16.10.0 || >=18.0.0"
  }
