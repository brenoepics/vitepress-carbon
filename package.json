{
  "name": "vitepress-carbon",
  "version": "1.3.3",
  "description": "Carbon embraces GitHub's monochromatic ethos, offering a theme for VitePress documentation that is sleek, modern, and effortlessly stylish.",
  "keywords": [
    "vue",
    "design-system"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm --filter vitepress-carbon-demo run dev",
    "demo-build": "pnpm --filter vitepress-carbon-demo run build",
    "lint": "prettier --check --write --parser typescript \"{__tests__,docs,packages,types}/**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"{__tests__,docs,packages,types}/**/*.ts\""
  },
  "author": "Breno A.",
  "license": "MIT",
  "homepage": "https://github.com/brenoepics/vitepress-carbon#readme",
  "bugs": {
    "url": "https://github.com/brenoepics/vitepress-carbon/issues"
  },
  "devDependencies": {
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@algolia/client-search": "^5.7.0",
    "@types/node": "^22.7.4",
    "search-insights": "^2.17.2",
    "vite": "^5.4.8",
    "vitepress": "^1.3.4"
  },
  "peerDependencies": {
    "vitepress": "^1.3.4"
  },
  "packageManager": "pnpm@9.9.0"
}
